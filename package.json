    "parametrized",
    "parametrize"
  ],
  "bugs": {
    "url": "https://github.com/skozin/mocha-vary/issues"
  },
  "homepage": "https://github.com/skozin/mocha-vary",
  "dependencies": {
    "arrow-mocha": "^1.0.1"
  },
  "devDependencies": {
    "babel": "^4.7.9",
    "chai": "^2.2.0",
    "grequire": "^1.0.0",
    "mocha": "^2.2.1"
  },
  "files": [
    "index.js",
    "es6/index.js"
  ],
